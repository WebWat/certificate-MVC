@model HttpErrorViewModel
@{
    ViewData["Title"] = Model.Title;
    Layout = "/Areas/Identity/Pages/Account/_Layout.cshtml";
}
<div class="form-signin">
    <div class="text-center mb-4">
        <h1 class="h1 mb-3 font-weight-normal text-danger" style="font-size:7em;">@Model.Error</h1>
        <h5>@Model.Description</h5>
        <a asp-controller="Home" asp-action="Index" class="mt-3">@Model.Back</a>
    </div>
    <partial name="_ApplicationNamePartial" />
</div>
